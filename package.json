{
    "name": "BrightByte",
    "version": "0.0.1",
    "author": "Tribalyte Technologies",
    "homepage": "",
    "private": true,
    "scripts": {
        "start": "mkdir -p www; cp config.xml www; npm run ionic:serve",
        "clean": "ionic-app-scripts clean",
        "build": "ionic-app-scripts build",
        "build-browser-release": "ionic cordova build browser --prod --release",
        "ionic:build": "ionic-app-scripts build",
        "ionic:serve": "ionic-app-scripts serve",
        "browser": "ionic cordova run browser",
        "lint": "IONIC_TYPE_CHECK_ON_LINT=true IONIC_BAIL_ON_LINT_ERROR=true ionic-app-scripts lint",
        "truffle:migrate": "truffle migrate --reset",
        "truffle:develop": "truffle develop",
        "truffle:custom-deploy": "ACCOUNT_PASSWORD=$ACCOUNT_PASSWORD truffle migrate --network custom --reset",
        "html-lint": "htmlhint src",
        "scss-lint": "sass-lint -v",
        "linters-ci": "npm run html-lint && npm run scss-lint && npm run lint",
        "prepush": "npm run linters-ci"
    },
    "dependencies": {
        "@angular/common": "5.2.11",
        "@angular/compiler": "5.2.11",
        "@angular/core": "5.2.11",
        "@angular/forms": "5.2.11",
        "@angular/http": "5.2.11",
        "@angular/platform-browser": "5.2.11",
        "@angular/platform-browser-dynamic": "5.2.11",
        "@machinomy/types-ethereumjs-tx": "0.1.0",
        "@ngx-translate/core": "8.0.0",
        "@ngx-translate/http-loader": "1.0.0",
        "@types/ethereumjs-tx": "1.0.0",
        "cordova-browser": "5.0.3",
        "cordova-plugin-device": "2.0.2",
        "cordova-plugin-whitelist": "1.3.3",
        "ethereumjs-tx": "1.3.4",
        "ionic-angular": "3.9.2",
        "ionicons": "3.0.0",
        "rxjs": "5.5.11",
        "sw-toolbox": "3.6.0",
        "truffle-contract": "3.0.5",
        "web3": "1.0.0-beta.34",
        "websocket": "1.0.26",
        "zone.js": "0.8.18"
    },
    "devDependencies": {
        "@angular/compiler-cli": "5.2.11",
        "@ionic/app-scripts": "3.1.8",
        "@types/node": "10.1.3",
        "codelyzer": "4.4.2",
        "htmlhint": "0.9.13",
        "ionic": "4.0.0-rc.6",
        "path": "0.12.7",
        "sass-lint": "1.12.1",
        "truffle": "4.1.9",
        "types-ethereumjs-util": "0.0.8",
        "typescript": "2.4.2",
        "husky": "0.14.3"
    },
    "description": "A Blockchain project",
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-plugin-device": {}
        },
        "platforms": [
            "browser"
        ]
    }
}
