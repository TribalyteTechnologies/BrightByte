{
    "name": "BrightByte",
    "version": "0.0.1",
    "author": "Tribalyte Technologies",
    "homepage": "",
    "private": true,
    "engines": {
        "node": "12",
        "yarn": "1"
    },
    "scripts": {
        "start": "shx mkdir www; shx cp config.xml www; yarn run ionic:serve",
        "clean": "ionic-app-scripts clean",
        "build": "ionic-app-scripts build",
        "prebuild-browser-release": "node version-sync.js",
        "build-browser-release": "ionic cordova build browser --prod --release",
        "ionic:build": "ionic-app-scripts build",
        "ionic:serve": "ionic-app-scripts serve",
        "browser": "ionic cordova run browser",
        "lint": "IONIC_TYPE_CHECK_ON_LINT=true IONIC_BAIL_ON_LINT_ERROR=true ionic-app-scripts lint",
        "html-lint": "htmlhint src",
        "scss-lint": "sass-lint -v --max-warnings 0",
        "linters-ci": "yarn run html-lint && yarn run scss-lint && yarn run lint",
        "prepush": "yarn run linters-ci",
        "preinstall": "rm -fr node_modules/*/node_modules/*/.git/",
        "postlinters-ci": "cd ../backend; yarn install; yarn run linters-ci",
        "wait:start": "yarn install; yarn run start & yarn run wait:webapp",
        "wait:webapp": "wait-on http://localhost:8100"
    },
    "dependencies": {
        "@angular/common": "5.2.11",
        "@angular/compiler": "5.2.11",
        "@angular/core": "5.2.11",
        "@angular/forms": "5.2.11",
        "@angular/http": "5.2.11",
        "@angular/platform-browser": "5.2.11",
        "@angular/platform-browser-dynamic": "5.2.11",
        "@machinomy/types-ethereumjs-tx": "0.1.0",
        "@ngx-translate/core": "9.0.2",
        "@ngx-translate/http-loader": "2.0.1",
        "@types/ethereumjs-tx": "1.0.0",
        "ajv": "6.10.0",
        "ajv-keywords": "3.4.0",
        "cordova-browser": "5.0.3",
        "cordova-plugin-device": "2.0.3",
        "cordova-plugin-whitelist": "1.3.4",
        "ethereum-cryptography": "^0.1.2",
        "ethereumjs-tx": "1.3.4",
        "ionic-angular": "3.9.10",
        "ionicons": "3.0.0",
        "jw-angular-social-buttons": "1.0.0",
        "ng-socket-io": "0.2.4",
        "readable-stream": "2.3.7",
        "rxjs": "5.5.11",
        "sw-toolbox": "3.6.0",
        "web3": "1.2.1",
        "websocket": "1.0.26",
        "zone.js": "0.8.18"
    },
    "devDependencies": {
        "@angular/compiler-cli": "5.2.11",
        "@ionic/app-scripts": "3.2.4",
        "@types/node": "10.1.3",
        "codelyzer": "4.4.4",
        "htmlhint": "0.9.13",
        "husky": "0.14.3",
        "ionic": "5.4.13",
        "path": "0.12.7",
        "sass-lint": "1.12.1",
        "shx": "0.3.2",
        "types-ethereumjs-util": "0.0.8",
        "typescript": "2.4.2",
        "utf8": "3.0.0",
        "wait-on": "3.3.0",
        "xml2json": "0.11.2",
        "yarn": "1.22.5"
    },
    "description": "A Blockchain project",
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-plugin-device": {}
        },
        "platforms": [
            "browser"
        ]
    }
}
